{
  "name": "quill-auto-detect-url",
  "version": "0.2.1",
  "description": "quill auto detect url",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "lib",
  "type": "module",
  "repository": "https://github.com/juyeong1260/quill-auto-detect-url",
  "author": "juyeong1260",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:js | yarn build:tsc",
    "build:js": "node ./esbuild.config.mjs",
    "build:tsc": "tsc",
    "lint": "eslint --config eslint.config.js ./src --report-unused-disable-directives --max-warnings 0"
  },
  "keywords": [
    "quill",
    "url",
    "links"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib",
    "package.json",
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "devDependencies": {
    "@eslint/compat": "^1.0.1",
    "@eslint/js": "^9.3.0",
    "esbuild": "^0.21.3",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "parchment": "^3.0.0",
    "prettier": "^3.2.5",
    "quill": "^2.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "peerDependencies": {
    "quill": ">=2",
    "quill-detla": ">=5"
  },
  "dependencies": {
    "normalize-url": "^8.0.1"
  },
  "packageManager": "yarn@4.4.1"
}
